pane_frames false

layout {
  pane split_direction="vertical" {
    pane size="60%"

    pane split_direction="horizontal" {
      pane command="npm" start_suspended=true {
        args "run" "format"
      }

      pane command="npm" split_direction="horizontal" {
        args "run" "dev"
      }
    }
  }
}
